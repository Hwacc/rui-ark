{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "",
    "target": "ES2020",
    "rootDir": "./src",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "declaration": true,
    "declarationMap": false,
    "noEmit": false,
    "outDir": "./dist",
    "sourceMap": false,
    "preserveSymlinks": true,
    "skipLibCheck": true
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules", "dist", "./tailwind.config.ts"]
}
