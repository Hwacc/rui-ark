{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "tsBuildInfoFile": "node_modules/.cache/.tsbuildinfo",
    "rootDir": "./src",
    "allowImportingTsExtensions": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "declaration": true,
    "declarationMap": false,
    "emitDeclarationOnly": false,
    "noEmit": false,
    "outDir": "./dist"
  },
  "paths": {},
  "include": ["src/**/*.ts"],
  "exclude": [
    "node_modules",
    "dist",
    "src/*.stories.ts",
    "src/examples"
  ]
}
