{
  "name": "@rark-ui/vue-addons-virtual",
  "type": "module",
  "version": "1.1.0",
  "packageManager": "pnpm@10.13.1",
  "description": "Virtual components for Rark UI",
  "author": {
    "name": "Hwacc",
    "email": "mshcccck@gmail.com",
    "url": "https://github.com/Hwacc/rark-ui"
  },
  "license": "MIT",
  "keywords": [
    "rark-ui",
    "vue",
    "addons",
    "virtual"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./index.css": "./dist/index.css"
  },
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "copy:css": "bun -e \"import { mkdir } from 'node:fs/promises'; await mkdir('./dist', { recursive: true }); await Bun.write('./dist/index.css', Bun.file('./src/index.css'));\"",
    "build": "rslib build --tsconfig ./tsconfig.build.json && vue-tsc --project tsconfig.build.json && tsc-alias --project tsconfig.build.json && bun run copy:css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "peerDependencies": {
    "vue": "^3.5.27"
  },
  "dependencies": {
    "@rark-ui/vue-addons-shared": "^1.0.0",
    "@tanstack/vue-virtual": "^3.13.18",
    "es-toolkit": "^1.44.0",
    "lucide-vue-next": "^0.563.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@rsbuild/plugin-babel": "^1.0.6",
    "@rsbuild/plugin-vue-jsx": "^1.1.1",
    "@rslib/core": "0.19.4",
    "rsbuild-plugin-unplugin-vue": "^0.1.0",
    "tsc-alias": "^1.8.16",
    "vue-tsc": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
