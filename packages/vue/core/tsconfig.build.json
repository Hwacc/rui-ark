{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "tsBuildInfoFile": "node_modules/.cache/.tsbuildinfo",
    "paths": {
      "@rui-ark/vue/*": ["./src/*"]
    },
    "declaration": true,
    "declarationMap": false,
    "emitDeclarationOnly": true,
    "noEmit": false,
    "outDir": "./dist"
  },
  "exclude": [
    "node_modules",
    "dist",
    "src/**/*.stories.ts",
    "src/**/examples/**"
  ]
}
